{
    "manifest_version": 3,
    "default_locale": "zh_CN",
    "name": "Another New Tab",
    "version": "0.0.1",
    "description": "A new tab with quotes and poems",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "host_permissions": [
        "https://v2.jinrishici.com/",
        "https://v1.hitokoto.cn/"
    ]
}
